{% extends 'base.html.twig' %}

{% block title %}Hello UserController!{% endblock %}

{% block body %}
{% for user in users %}
      <img src="{{ asset('uploads/images/' ~ user.image) }}" alt="User Image">

<p><strong>Username:</strong> {{ user.username }}</p> 
    <p><strong>Email:</strong> {{ user.email }}</p> 
{% endfor %}

{% endblock %}
